<%= simple_form_for [@annoyance, @booking] do |f| %>
  <p>Please include your availability</p>
  <%= f.input :target %>
  <%= f.input :info %>
  <%= f.input :starting_time %>
  <%= f.input :ending_time %>
  <%= f.submit class: 'btn-ghost' %>
<% end %>

<div>
<%= link_to "List of Bookings", annoyance_bookings_path(@annoyance), class: 'btn-ghost' %>
<%= link_to "Go back", annoyances_path, class: 'btn-ghost'%>
</div>
